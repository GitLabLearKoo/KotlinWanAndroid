<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <app.itgungnir.kwa.common.widget.head_bar.HeadBar
        android:id="@+id/headBar"
        style="@style/HeadBarStyle"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        app:hb_menuBackground="@color/colorPrimary"
        app:hb_menuIconColor="@color/text_color_head_bar"
        app:hb_menuTitleColor="@color/text_color_head_bar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <app.itgungnir.kwa.common.widget.browser.WebBrowser
        android:id="@+id/browserView"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/headBar" />

</androidx.constraintlayout.widget.ConstraintLayout>